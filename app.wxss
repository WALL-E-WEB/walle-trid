/**app.wxss**/


.container {
  height: 100%;
  
  box-sizing: border-box;
}

view, text {
  box-sizing: border-box;
  font-size: 24rpx;
}

.textline {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  word-break: break-all;
}

.textlineT {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp:2;
  line-clam:2;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.bg {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
